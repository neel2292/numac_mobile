<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="100" id="com.erpx.numac" version="0.0.14" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Numac Mobile</name>
    <description>
		Numac Mobile Client for ERPNext
	</description>
    <author email="dev.erpx@gmail.com" href="">
		ERPX-X
	</author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <platform name="android">
        <icon density="ldpi" src="res/android/icons/ldpi.png" />
        <icon density="mdpi" src="res/android/icons/mdpi.png" />
        <icon density="hdpi" src="res/android/icons/hdpi.png" />
        <icon density="xhdpi" src="res/android/icons/xhdpi.png" />
        <icon density="xxhdpi" src="res/android/icons/xxhdpi.png" />
        <icon density="xxxhdpi" src="res/android/icons/xxxhdpi.png" />
        <splash density="land-hdpi" src="res/android/splash/port-hdpi.png" />
        <splash density="land-ldpi" src="res/android/splash/port-ldpi.png" />
        <splash density="land-mdpi" src="res/android/splash/port-mdpi.png" />
        <splash density="land-xhdpi" src="res/android/splash/port-xhdpi.png" />
        <splash density="land-xxhdpi" src="res/android/splash/port-xxxhdpi.png" />
        <splash density="land-xxxhdpi" src="res/android/splash/port-xxxhdpi.png" />
        <splash density="port-hdpi" src="res/android/splash/port-hdpi.png" />
        <splash density="port-ldpi" src="res/android/splash/port-ldpi.png" />
        <splash density="port-mdpi" src="res/android/splash/port-mdpi.png" />
        <splash density="port-xhdpi" src="res/android/splash/port-xhdpi.png" />
        <splash density="port-xxxhdpi" src="res/android/splash/port-xxxhdpi.png" />
    </platform>
    <platform name="ios">
        <preference name="StatusBarOverlaysWebView" value="false" />
        <preference name="StatusBarBackgroundColor" value="#ffffff" />
        <preference name="StatusBarStyle" value="default" />
        <preference name="Orientation" value="all" />
        <icon height="57" src="res/ios/icon/icon.png" width="57" />
        <icon height="114" src="res/ios/icon/icon@2x.png" width="114" />
        <icon height="40" src="res/ios/icon/icon-40.png" width="40" />
        <icon height="80" src="res/ios/icon/icon-40@2x.png" width="80" />
        <icon height="120" src="res/ios/icon/icon-40@3x.png" width="120" />
        <icon height="50" src="res/ios/icon/icon-50.png" width="50" />
        <icon height="100" src="res/ios/icon/icon-50@2x.png" width="100" />
        <icon height="60" src="res/ios/icon/icon-60.png" width="60" />
        <icon height="120" src="res/ios/icon/icon-60@2x.png" width="120" />
        <icon height="180" src="res/ios/icon/icon-60@3x.png" width="180" />
        <icon height="72" src="res/ios/icon/icon-72.png" width="72" />
        <icon height="144" src="res/ios/icon/icon-72@2x.png" width="144" />
        <icon height="76" src="res/ios/icon/icon-76.png" width="76" />
        <icon height="152" src="res/ios/icon/icon-76@2x.png" width="152" />
        <icon height="167" src="res/ios/icon/icon-83.5@2x.png" width="167" />
        <icon height="29" src="res/ios/icon/icon-small.png" width="29" />
        <icon height="58" src="res/ios/icon/icon-small@2x.png" width="58" />
        <icon height="87" src="res/ios/icon/icon-small@3x.png" width="87" />
        <icon height="1024" src="res/ios/icon/icon-1024.png" width="1024" />
        <splash height="480" src="res/ios/splash/Default.png" width="320" />
        <splash height="960" src="res/ios/splash/Default@2x.png" width="640" />
        <splash height="1024" src="res/ios/splash/Default-Portrait.png" width="768" />
        <splash height="2048" src="res/ios/splash/Default-Portrait@2x.png" width="1536" />
        <splash height="768" src="res/ios/splash/Default-Landscape.png" width="1024" />
        <splash height="1536" src="res/ios/splash/Default-Landscape@2x.png" width="2048" />
        <splash height="1136" src="res/ios/splash/Default-568h@2x.png" width="640" />
        <splash height="1334" src="res/ios/splash/Default-667h@2x.png" width="750" />
        <splash height="2208" src="res/ios/splash/Default-736h.png" width="1242" />
        <splash height="1242" src="res/ios/splash/Default-Landscape-736h.png" width="2208" />
    </platform>
    <plugin name="cordova-plugin-statusbar" spec="^2.4.3" />
    <plugin name="cordova-plugin-inappbrowser" spec="^3.1.0" />
    <plugin name="cordova-plugin-file" spec="^6.0.2" />
    <plugin name="phonegap-plugin-barcodescanner" spec="^8.1.0">
        <variable name="ANDROID_SUPPORT_V4_VERSION" value="27.+" />
    </plugin>
    <plugin name="cordova-plugin-sunmi-inner-printer" spec="https://github.com/tmalbonph/cordova-plugin-sunmi-inner-printer">
        <variable name="ANDROID_SUPPORT_V13_VERSION" value="27.+" />
    </plugin>
    <engine name="android" spec="^7.1.4" />
</widget>
